{
    "name": "Chorme Material Start",
    "description": "Chrome material design new tab page",
    "version": "0.0.0.5",
    "manifest_version": 2,
    "offline_enabled": true,
    "permissions": [
        "bookmarks",
        "topSites",
        "storage",
        "chrome://favicon/",
        "http://*/*",
        "https://*/*",
    ],
    "chrome_url_overrides" : { "newtab": "main.html" },
    "options_page": "options.html"
}
