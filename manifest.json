{
    "name": "Chorme Material Start - Beta",
    "short_name": "Material Start - Beta",
    "description": "Chrome material design new tab page - Beta version",
    "version": "0.7",
    "manifest_version": 2,
    "offline_enabled": true,
    "permissions": [
        "bookmarks",
        "topSites",
        "storage",
        "chrome://favicon/",
        "https://*/*",
        "http://*/*"
    ],
    "chrome_url_overrides" : { "newtab": "main.html" },
    "options_page": "options.html"
}
