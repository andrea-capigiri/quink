{
    "name": "Quink Lite - New Tab",
    "short_name": "Quink Lite",
    "description": "Quink soubstitute your new tab with a smart bookmark list & search.",
    "version": "1.0",
    "manifest_version": 2,
    "offline_enabled": true,
    "permissions": [
        "bookmarks",
        "topSites",
        "storage",
        "chrome://favicon/",
        "https://*/*",
        "http://*/*"
    ],
    "chrome_url_overrides" : { "newtab": "main.html" }
}
