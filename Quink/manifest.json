{
	"name": "Quink - Your quick bookmark new tab",
	"short_name": "Quink",
	"description": "Quink - Your quick bookmark new tab",
	"version": "2.1",

	"offline_enabled": true,
	"manifest_version": 3,
	"permissions": ["bookmarks", "topSites", "storage", "favicon"],
	"chrome_url_overrides": {
		"newtab": "main.html"
	},
	"web_accessible_resources": [
		{
			"resources": ["_favicon/*"],
			"matches": ["<all_urls>"],
			"extension_ids": ["*"]
		}
	]
}
