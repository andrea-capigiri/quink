{
  "name": "Quink - Your quick bookmark new tab",
  "short_name": "Quink",
  "description": "Quink - Your quick bookmark new tab",
  "version": "3.1.4",
  "icons": {
    "16": "assets/logos/icon_light_16.png",
    "48": "assets/logos/icon_light_48.png",
    "128": "assets/logos/icon_light_128.png"
  },
  "theme_icons": [
    {
      "light": "assets/logos/icon_light_16.png",
      "dark": "assets/logos/icon_dark_16.png",
      "size": 16
    },
    {
      "light": "assets/logos/icon_light_48.png",
      "dark": "assets/logos/icon_dark_48.png",
      "size": 48
    },
    {
      "light": "assets/logos/icon_light_128.png",
      "dark": "assets/logos/icon_dark_128.png",
      "size": 128
    }
  ],
  "offline_enabled": true,
  "manifest_version": 3,
  "permissions": [
    "favicon",
    "bookmarks"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "_favicon/*" ],
      "matches": [ "<all_urls>" ],
      "extension_ids": [ "*" ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}