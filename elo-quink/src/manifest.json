{
    "name": "Quink - Your quick bookmark new tab",
    "short_name": "Quink",
    "description": "Quink - Your quick bookmark new tab",
    "version": "3.1.0",

    "offline_enabled": true,
    "manifest_version": 3,
    "permissions": [
        "favicon",
        "bookmarks",
        "topSites",
        "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
      }
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    }

  }